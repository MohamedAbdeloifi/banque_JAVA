CREATE DATABASE banque;
USE banque;

CREATE TABLE titulaire(
	CODETITULAIRE INT,
	PRENOMTITULAIRE VARCHAR(32),
	NOMTITULAIRE VARCHAR(32),
	ADRESSETITULAIRE VARCHAR(32),
	CODEPOSTALTITULAIRE CHAR(5),
	PRIMARY KEY (CODETITULAIRE)
)ENGINE=INNODB;

CREATE TABLE typecompte(
	CODETYPECOMPTE CHAR(1),
	INTITULECOMPTE VARCHAR(25),
	PRIMARY KEY (CODETYPECOMPTE)
)ENGINE=INNODB;

CREATE TABLE compte(
	NUMEROCOMPTE INT,
	CODETYPECOMPTE CHAR(1),
	CODETITULAIRE INT,
	SOLDECOMPTE FLOAT(10,2),
	PRIMARY KEY (NUMEROCOMPTE),
	FOREIGN KEY (CODETITULAIRE) REFERENCES titulaire 	(CODETITULAIRE),
	FOREIGN KEY (CODETYPECOMPTE) REFERENCES  typecompte 	(CODETYPECOMPTE)
)ENGINE=INNODB;

CREATE TABLE operations (
	NUMEROOPERATION INT,
NUMEROCOMPTE INT,
	DATEOPERATION DATE,
	LIBELLEOPERATION VARCHAR(32),
MONTANT FLOAT(10,2),
	TYPEOPERATION CHAR(1),
	PRIMARY KEY (NUMEROOPERATION),
	FOREIGN KEY (NUMEROCOMPTE) REFERENCES compte (NUMEROCOMPTE)
)ENGINE=INNODB;

